Soma = 0
for(int i=0; i<10; i++)
    soma += i;

// soma começa com 0
// i começa com 0
// troca 0 por 0, encrementa o 1 que se torna 1
// o passa a 0, 0, 1, 3, 6...
// 1 passa a 1, 2, 3...

int fsoma (int n){
    Soma = 0
    for(int i=0; i<10; i++)
        soma += i;
    return soma;
    }

